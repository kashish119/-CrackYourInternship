DAY-1   03|05|2022
1) Medium -- 75. Sort Colors

  class Solution {
  public:
    void sortColors(vector<int>& nums) 
    {
        int n,i,j,temp;
        for(i=0;i<nums.size()-1;i++)
        {
            for(j=0;j<nums.size()-i-1;j++)
            {
               if(nums[j] > nums[j+1])
                {
                    temp = nums[j];
                    nums[j] = nums[j+1];
                    nums[j+1] = temp;
                }   
            }
        }

    }
};

