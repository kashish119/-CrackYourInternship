  Easy-- 217. Contains Duplicate
    
    class Solution  
    {
     public:
      bool containsDuplicate(vector<int>& nums) 
      {
        bool flag=0;
          sort(nums.begin(),nums.end());
          for(int i=1;i<nums.size();i++)  
          {
                if(nums[i]==nums[i-1])
                    flag=1;
          }
        return flag;
 
      }
    
  };
  
 Easy-- 217. Contains Duplicate 
 
  class Solution 
  {
   public:
    bool containsDuplicate(vector<int>& nums) 
    {
       bool flag=0;
        sort(nums.begin(),nums.end());
    

        for(int i=1;i<nums.size();i++){
                if(nums[i]==nums[i-1])
                    flag=1;
            }
        return flag;
 
    }
    
};


Easy -- 53. Maximum Subarray

class Solution 
{
  public:
    int maxSubArray(vector<int>& nums)
    {
        int sum=0;
        int maximum=INT_MIN;
        for(auto it:nums)
        {
            sum+=it;
            maximum=max(sum,maximum);
            if(sum<0)
                sum=0;
        }
        return maximum;
    }
};

